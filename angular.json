{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ezhuthupizhai": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ezhuthupizhai/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/vendor/wow/wow.min.js",
              "src/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/vendor/bootstrap-select/dist/js/bootstrap-select.min.js",
              "src/assets/vendor/magnific-popup/magnific-popup.js",
              "src/assets/vendor/swiper/swiper-bundle.min.js",
              "src/assets/vendor/masonry/masonry-4.2.2.js",
              "src/assets/vendor/masonry/isotope.pkgd.min.js",
              "src/assets/vendor/wnumb/wNumb.js",
              "src/assets/vendor/countdown/jquery.countdown.js",
              "src/assets/js/dz.carousel.js",
              "src/assets/vendor/counter/waypoints-min.js",
              "src/assets/vendor/counter/counterup.min.js",
              "src/assets/vendor/lightgallery/dist/lightgallery.min.js",
              "src/assets/vendor/lightgallery/dist/plugins/thumbnail/lg-thumbnail.min.js",
              "src/assets/vendor/lightgallery/dist/plugins/zoom/lg-zoom.min.js",
              "src/assets/vendor/group-slide/group-loop.js",
              "src/assets/js/dz.ajax.js",
              "src/assets/js/custom.js"
            ],
            "allowedCommonJsDependencies": [
              "ev-emitter/ev-emitter",
              "outlayer"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "40kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "production": {
              "sourceMap": true,
              "outputHashing": "all",
              "optimization": true,
              "buildOptimizer": true,
              "vendorChunk": false,
              "extractLicenses": true,
              "namedChunks": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ezhuthupizhai:build:production"
            },
            "development": {
              "buildTarget": "ezhuthupizhai:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ezhuthupizhai:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ezhuthupizhai/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "outputHashing": "media",
              "optimization": true,
              "sourceMap": true,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "ezhuthupizhai:build:development",
              "serverTarget": "ezhuthupizhai:server:development"
            },
            "production": {
              "browserTarget": "ezhuthupizhai:build:production",
              "serverTarget": "ezhuthupizhai:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "ezhuthupizhai:build:production",
              "serverTarget": "ezhuthupizhai:server:production"
            },
            "development": {
              "browserTarget": "ezhuthupizhai:build:development",
              "serverTarget": "ezhuthupizhai:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": "b98d4715-4904-43d6-a151-15227919183d"
  }
}